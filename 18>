class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введите свой возраст: ");
            int age = Convert.ToInt32(Console.ReadLine());

            if (age > 18)
            {
                Console.WriteLine("Доступ разрешен!");
            }
            else
            {
                Console.WriteLine("Доступ запрещен!");
            }
        }
    }
